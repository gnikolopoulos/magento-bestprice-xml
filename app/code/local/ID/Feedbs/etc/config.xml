<?xml version="1.0"?>
  <config>
    <modules>
      <ID_Feedbs>
        <version>0.0.1</version>
      </ID_Feedbs>
    </modules>

    <frontend>
      <routers>
        <feedbs>
          <use>standard</use>
          <args>
            <module>ID_Feedbs</module>
            <frontName>feedbs</frontName>
          </args>
        </feedbs>
      </routers>
    </frontend>
      
    <global>
      <helpers>
        <feedbs>
          <class>ID_Feedbs_Helper</class>
        </feedbs>
      </helpers>
    </global>
  </config>